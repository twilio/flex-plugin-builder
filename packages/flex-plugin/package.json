    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.0.3"
  },
  "gitHead": "ca02b3d214e6dfa4277b6b69cbbb178783004edf",
  "dependencies": {
    "chalk": "^2.4.1",
    "common-tags": "^1.8.0",
    "ncp": "^2.0.0"
  },
  "peerDependencies": {
    "@twilio/flex-ui": "*"
  },
  "jest": {
    "preset": "ts-jest",
    "testURL": "https://flex.twilio.com/admin",
    "testMatch": [
      "<rootDir>/__tests__/**/*.ts",
