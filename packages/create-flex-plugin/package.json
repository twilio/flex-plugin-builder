{
  "name": "create-flex-plugin",
  "version": "2.3.0",
  "description": "Toolkit to create a new Twilio Flex Plugin",
  "keywords": [
    "flex",
    "twilio",
    "plugin"
  ],
  "author": "Dominik Kundel <dkundel@twilio.com>",
  "homepage": "https://github.com/twilio/flex-plugin-builder#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "create-flex-plugin": "bin/create-flex-plugin"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "bin",
    "dist",
    "templates"
