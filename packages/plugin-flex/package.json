{
  "name": "@twilio-labs/plugin-flex",
  "version": "1.3.1-beta.0",
  "description": "Create, develop and deploy Flex plugins using the Twilio CLI",
  "keywords": [
    "twilio",
    "flex",
    "twilio flex",
    "flex plugins",
    "flex plugins cli",
    "flex plugins api",
    "oclif-plugin"
  ],
  "homepage": "https://github.com/twilio-labs/plugin-flex",
  "bugs": "https://github.com/twilio-labs/plugin-flex/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/plugin-flex.git"
  },
  "license": "MIT",
  "author": "Twilio @twilio",
  "files": [
    "/oclif.manifest.json",
    "/lib",
    "/package-lock.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "clean": "rm -rf lib node_modules",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "oclif-dev manifest && npm run replace-doc",
    "postpack": "rm -f oclif.manifest.json",
    "publish:alpha": "./bin/publish alpha $1",
    "publish:beta": "./bin/publish beta $1",
    "publish:public": "./bin/publish public $1",
    "replace-doc": "cp docs/README.md README.md && oclif-dev readme",
    "test": "mocha -r ts-node/register \"src/__tests__/**/*.test.ts\"",
    "test:ci": "nyc npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/parser": "^3.8.5",
    "@twilio/cli-core": "^5.15.1",
    "create-flex-plugin": "4.3.18-beta.0",
    "dayjs": "^1.10.2",
    "fancy-test": "^1.4.10",
    "flex-plugin-scripts": "4.3.18-beta.0",
    "flex-plugins-api-client": "^0.21.1",
    "flex-plugins-api-toolkit": "^0.21.1",
    "flex-plugins-utils-exception": "^0.18.0",
    "flex-plugins-utils-logger": "^0.18.0",
    "flex-plugins-utils-spawn": "^0.18.2",
    "lodash.startcase": "^4.4.0",
    "mkdirp": "^1.0.4",
    "package-json": "^6.5.0",
    "phin": "^3.5.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@twilio/cli-test": "^2.1.1",
    "@types/chai": "^4.2.14",
    "@types/lodash.startcase": "^4.4.6",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "@types/yargs": "^15.0.12",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "eslint-config-twilio-mocha": "^1.31.0",
    "eslint-config-twilio-ts": "^1.31.0",
    "globby": "^11.0.2",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.1.1",
    "twilio": "^3.54.2",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "oclif": {
    "name": "flex",
    "commands": "./lib/commands",
    "bin": "twilio",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "flex": {
        "description": "Create, develop and deploy Flex plugins using the Twilio CLI"
      }
    }
  }
}
