{
  "name": "@twilio-labs/plugin-flex",
  "version": "0.1.6",
  "description": "Create, develop and deploy Flex plugins using the Twilio CLI",
  "keywords": [
    "oclif-plugin"
  ],
  "homepage": "https://github.com/twilio-labs/plugin-flex",
  "bugs": "https://github.com/twilio-labs/plugin-flex/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/plugin-flex.git"
  },
  "license": "MIT",
  "author": "Twilio @twilio",
  "files": [
    "/oclif.manifest.json",
    "/lib",
    "/package-lock.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test:ci": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "test": "mocha -r ts-node/register \"src/__tests__/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/parser": "^3.8.5",
    "@twilio/cli-core": "5.5.0",
    "create-flex-plugin": "4.3.1-alpha.0",
    "fancy-test": "^1.4.8",
    "flex-plugin-scripts": "4.3.1-alpha.0",
    "flex-plugins-api-client": "^0.11.0",
    "flex-plugins-api-toolkit": "^0.11.0",
    "flex-plugins-utils-exception": "^0.9.0",
    "flex-plugins-utils-logger": "^0.9.0",
    "semver": "^7.3.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@twilio/cli-test": "^2.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/semver": "^7.2.0",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-twilio-mocha": "^1.26.0",
    "eslint-config-twilio-ts": "^1.26.0",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "name": "flex",
    "commands": "./lib/commands",
    "bin": "twilio",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "flex": {
        "description": "Create, develop and deploy Flex plugins using the Twilio CLI"
      }
    }
  }
}
