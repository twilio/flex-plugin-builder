{
  "name": "flex-plugin-builder",
  "private": true,
  "homepage": "https://github.com/twilio/flex-plugin-builder#readme",
  "bugs": {
    "url": "https://github.com/twilio/flex-plugin-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio/flex-plugin-builder.git"
  },
  "license": "MIT",
  "contributors": [
    "Dominik Kundel <dkundel@twilio.com>",
    "Kousha Talebian <ktalebian@twilio.com>"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "audit": "lerna-audit",
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "lerna run build",
    "coverage": "codecov --disable=gcov",
    "docs": "./bin/docs-generator.js",
    "postinstall": "npm run bootstrap",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "prepare": "husky install",
    "publish": "./bin/publish",
    "release:alpha": "lerna-travis-release alpha",
    "release:beta": "lerna-travis-release beta",
    "release:public": "lerna-travis-release public",
    "test": "./bin/test-runner.sh",
    "test:ci": "jest --clearCache && npm run test -- --no-cache --coverage",
    "test:one": "npm run test -- $@",
    "test:watch": "npm run test -- --watchAll"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@k88/lerna-travis": "^1.1.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-register": "^6.26.0",
    "codecov": "^3.8.1",
    "eslint": "^7.23.0",
    "eslint-config-twilio-ts": "^1.34.2",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "lerna-audit": "^1.3.1",
    "markdown-toc": "^1.2.0",
    "nyc": "^15.1.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "ultra-runner": "^3.10.5"
  }
}
